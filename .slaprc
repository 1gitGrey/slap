{
  "messageDuration": 5000,
  "blinkRate": 500,
  "editor": {
    "useSpaces": false,
    "tabSize": 4,
    "pageLines": 10,
    "doubleClickDuration": 600,
    "cursorPadding": {"top": 2, "left": 2, "right": 2, "bottom": 2},
    "style": {
      "selection": "{cyan-bg}",
      "match": "{yellow-bg}",
      "currentLine": "{blue-bg}",
      "matchingBracket": "{green-bg}{bold}",
      "mismatchedBracket": "{red-bg}{bold}",

      "keyword": "{red-fg}",
      "built_in": "{yellow-fg}",
      "preprocessor": "{red-fg}",

      "title": "{underline}",
      "params": "{bold}",

      "class": "",
      "function": "",
      "decorator": "{magenta-fg}",

      "shebang": "{yellow-bg}{black-fg}",
      "variable": "{yellow-fg}",
      "operator": "{green-fg}",
      "subst": "",

      "number": "{green-fg}{bold}",
      "string": "{green-fg}{bold}",
      "regexp": "{green-fg}{bold}",
      "literal": "{green-fg}{bold}",

      "comment": "{white-bg}{black-fg}",

      "header": "{bold}",
      "strong": "{bold}",
      "code": "{green-fg}",
      "link_label": "",
      "link_url": "{yellow-fg}"
    },
    "bindings": {
      "goLeft": "left",
      "goLeftWord": ["C-left", "M-b"],
      "goLeftInfinity": "home",
      "goRight": "right",
      "goRightWord": ["C-right", "M-f"],
      "goRightInfinity": "end",
      "goUp": "up",
      "goUpParagraph": ["C-up", "M-{"],
      "goUpPage": "pageup",
      "goUpInfinity": ["C-home", "M-<"],
      "goDown": "down",
      "goDownParagraph": ["C-down", "M-}"],
      "goDownPage": "pagedown",
      "goDownInfinity": ["C-end", "M->"],
      "goMatchingBracket": ["C-m", "\u001d"],

      "selectAll": "C-a",
      "selectLeft": "S-left",
      "selectLeftWord": "C-S-left",
      "selectLeftInfinity": "S-home",
      "selectRight": "S-right",
      "selectRightWord": "C-S-right",
      "selectRightInfinity": "S-end",
      "selectUp": "S-up",
      "selectUpParagraph": "C-S-up",
      "selectUpPage": "S-pageup",
      "selectUpInfinity": "C-S-home",
      "selectDown": "S-down",
      "selectDownParagraph": "C-S-down",
      "selectDownPage": "S-pagedown",
      "selectDownInfinity": "C-S-end",
      "selectMatchingBracket": "C-S-m",

      "deleteLeft": "backspace",
      "deleteRight": "delete",
      "deleteLeftWord": ["C-backspace", "C-w", "M-delete"],
      "deleteRightWord": ["C-delete", "M-d"],
      "deleteLeftInfinity": "C-S-backspace",
      "deleteRightInfinity": "C-S-delete",

      "copy": "C-c",
      "cut": "C-x",
      "paste": "C-v",
      "indent": ["tab", "C-tab"],
      "dedent": "S-tab",
      "undo": "C-z",
      "redo": "C-y",
      "toggleInsertMode": "insert"
    }
  },
  "field": {
    "height": 1,
    "cursorPadding": {"left": 2, "right": 2, "top": 0, "bottom": 0},
    "style": {
      "currentLine": "{blue-bg}{underline}"
    },
    "bindings": {
      "submit": "enter",
      "cancel": "escape"
    }
  },
  "style": {
    "main": "{magenta-bg}",
    "header": "",
    "changed": "{yellow-fg}{bold}",
    "info": "{blue-bg}{white-fg}",
    "success": "{green-bg}{white-fg}",
    "warning": "{yellow-bg}{black-fg}",
    "error": "{red-bg}{white-fg}",
    "blink": "{inverse}",
    "prompt": ""
  },
  "bindings": {
    "quit": ["escape", "C-q"],
    "save": "C-s",
    "saveAs": ["C-S-s", "M-S-s"],
    "find": "C-f",
    "goLine": "C-g"
  }
}
